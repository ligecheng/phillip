  <build>
  <plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
			<plugin>
				<!-- apache官方tomcat插件，可以在这里设置tomcat的端口，访问路径，编码方式等等 -->
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<version>2.2</version>
				<configuration>
		
					<!-- url:表示你要远程热部署的tomcat访问路径 -->
					<!-- server表示你在本地maven的setting.xml的server的id -->
					<!-- path则代表你的项目部署后的访问路径 -->
					<port>8080</port>
					<!-- <server>tomcat8</server> -->
					
				</configuration>
			</plugin>
			
			<plugin>
                  <groupId>org.mybatis.generator</groupId>
                <artifactId>mybatis-generator-maven-plugin</artifactId>
                <version>1.3.2</version>
                <configuration>
                    <verbose>true</verbose>
                    <overwrite>true</overwrite>
                </configuration>
            </plugin>
			
		</plugins>
